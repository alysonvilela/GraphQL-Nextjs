[32m+import axios from 'axios'[m
[32m+import { useState, useRef, useCallback } from 'react'[m
[32m+import * as S from './weather.style'[m
[32m+[m
[32m+interface DataProps {[m
[32m+  coord: { lon: number; lat: number }[m
[32m+  weather: [{ id: number; main: string; description: string; icon: string }][m
[32m+  base: number[m
[32m+  main: {[m
[32m+    temp: number[m
[32m+    feels_like: number[m
[32m+    temp_min: number[m
[32m+    temp_max: number[m
[32m+    pressure: number[m
[32m+    humidity: number[m
[32m+    sea_level: number[m
[32m+    grnd_level: number[m
[32m+  }[m
[32m+  visibility: number[m
[32m+  wind: { speed: number; deg: number; gust: number }[m
[32m+  clouds: { all: number }[m
[32m+  dt: number[m
[32m+  sys: {[m
[32m+    type: number[m
[32m+    id: number[m
[32m+    country: string[m
[32m+    sunrise: number[m
[32m+    sunset: number[m
[32m+  }[m
[32m+  timezone: number[m
[32m+  id: number[m
[32m+  name: string[m
[32m+  cod: number[m
[32m+}[m
[32m+[m
[32m+const WeatherApp = () => {[m
[32m+  const searchRef = useRef<HTMLInputElement>(null)[m
[32m+  const search = searchRef.current[m
[32m+  const [error, setError] = useState(false)[m
[32m+  const [data, setData] = useState<DataProps>()[m
[32m+  const [velocity, setVelocity] = useState(0)[m
[32m+  const [temperatures, setTemperatures] = useState({[m
[32m+    feelsLike: 0,[m
[32m+    temp: 0,[m
[32m+    temp_min: 0,[m
[32m+    temp_max: 0[m
[32m+  })[m
[32m+[m
[32m+  const handleClean = () => {[m
[32m+    if (search) {[m
[32m+      search.value = ''[m
[32m+    }[m
[32m+  }[m
[32m+[m
[32m+  const getTemperature = useCallback((value: number) => {[m
[32m+    const convert = Math.round(value - 273.15)[m
[32m+    return convert[m
[32m+  }, [])[m
[32m+[m
[32m+  const getApi = async () => {[m
[32m+    if (search?.value) {[m
[32m+      const { data: weatherData } = await axios.get<DataProps>([m
[32m+        `https://api.openweathermap.org/data/2.5/weather?q=${search.value}&appid=${process.env.appId}`[m
[32m+      )[m
[32m+      const KmHour = Math.round(weatherData.wind.speed * 3.6)[m
[32m+      setVelocity(KmHour)[m
[32m+      setTemperatures({[m
[32m+        feelsLike: getTemperature(weatherData.main.feels_like),[m
[32m+        temp: getTemperature(weatherData.main.temp),[m
[32m+        temp_min: getTemperature(weatherData.main.temp_min),[m
[32m+        temp_max: getTemperature(weatherData.main.temp_max)[m
[32m+      })[m
[32m+      setData(weatherData)[m
[32m+    }[m
[32m+  }[m
[32m+[m
[32m+  return ([m
[32m+    <S.Body>[m
[32m+      <S.Container>[m
[32m+        <S.Wrapper>[m
[32m+          <h1>Como est√° o clima? Nao tem filtro</h1>[m
[32m+          {data && ([m
[32m+            <S.Popup>[m
[32m+              <S.PopupFirstSection>[m
[32m+                <S.Top>[m
[32m+                  <S.PopupCity>[m
[32m+                    {data.name} - {data.sys.country}[m
[32m+                  </S.PopupCity>[m
[32m+                  <h2>[m
[32m+                    {temperatures.temp}¬∫C {data.weather[0].main}[m
[32m+                  </h2>[m
[32m+                  <S.MaxMins>[m
[32m+                    <span>{temperatures.temp_min}(min)</span>[m
[32m+                    <span>{temperatures.temp_max}(max)</span>[m
[32m+                  </S.MaxMins>[m
[32m+                </S.Top>[m
[32m+                <div>[m
[32m+                  <ul>[m
[32m+                    <li>[m
[32m+                      Sensa√ß√£o <span>{temperatures.feelsLike}¬∫C</span>[m
[32m+                    </li>[m
[32m+                    <li>[m
[32m+                      Vento <span>{velocity}km/h</span>[m
[32m+                    </li>[m
[32m+                    <li>[m
[32m+                      Umidade <span>{data.main.humidity}%</span>[m
[32m+                    </li>[m
[32m+                  </ul>[m
[32m+                </div>[m
[32m+              </S.PopupFirstSection>[m
[32m+            </S.Popup>[m
[32m+          )}[m
[32m+          <form[m
[32m+            onSubmit={(e) => {[m
[32m+              e.preventDefault()[m
[32m+              if (search !== null) {[m
[32m+                getApi()[m
[32m+              } else {[m
[32m+                console.log('oi')[m
[32m+                return setError(true)[m
[32m+              }[m
[32m+            }}[m
[32m+          >[m
[32m+            <label>[m
[32m+              <S.InputWrapper>[m
[32m+                <input[m
[32m+                  type="text"[m
[32m+                  placeholder="Insira aqui o nome da cidade"[m
[32m+                  ref={searchRef}[m
[32m+                />[m
[32m+                {error && <span>ocorreu um erro</span>}[m
[32m+              </S.InputWrapper>[m
[32m+            </label>[m
[32m+            <div className="buttons">[m
[32m+              <S.Button type="submit">Pesquisar</S.Button>[m
[32m+              <S.CleanButton[m
[32m+                onClick={() => {[m
[32m+                  handleClean()[m
[32m+                  setData(undefined)[m
[32m+                }}[m
[32m+              >[m
[32m+                Limpar[m
[32m+              </S.CleanButton>[m
[32m+            </div>[m
[32m+          </form>[m
[32m+        </S.Wrapper>[m
[32m+      </S.Container>[m
[32m+    </S.Body>[m
[32m+  )[m
[32m+}[m
[32m+[m
[32m+export default WeatherApp[m
